  "sprite": "https://gsi-cyberjapan.github.io/optimal_bvmap/sprite/std",
  "sources": {
    "v": {
      "type": "vector",
      "minzoom": 4,
      "maxzoom": 16,
      "url": "https://tunnel.optgeo.org/martin/bvmap",
