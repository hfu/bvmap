      "paint": {
        "line-color": "rgba(0,0,0,1)",
        "line-width": 1
      }
    },
    {
      "id": "水部表記線point",
      "type": "symbol",
      "source": "v",
      "source-layer": "WRltLine",
      "filter": [
        "==",
        ["geometry-type"],
        "Point"
      ],
      "layout": {
        "icon-allow-overlap": true,
        "icon-image": "流水方向",
        "icon-size": 1.2,
        "icon-rotate": ["get", "vt_angle"],
        "icon-rotation-alignment": "map"
      }
    },
    {
      "id": "地形表記面2",
      "type": "fill",
      "source": "v",
      "source-layer": "TpgphArea",
      "minzoom": 17,
      "filter": [
        "in",
        ["get", "vt_flag17"],
