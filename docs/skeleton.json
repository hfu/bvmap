      }
    },
    {
      "id": "水部表記線polygon",
      "type": "fill",
      "source": "v",
      "source-layer": "WRltLine",
      "filter": [
        "==",
        ["geometry-type"],
        "Polygon"
      ],
      "paint": {
        "fill-color": "rgba(255,255,255,0)",
        "fill-outline-color": "rgba(0,0,0,1)"
      }
    },
    {
      "id": "水部表記線line",
      "type": "line",
      "source": "v",
      "source-layer": "WRltLine",
